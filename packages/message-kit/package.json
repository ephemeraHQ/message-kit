{
  "name": "@xmtp/message-kit",
  "version": "1.2.33",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "yarn build -w",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "yarn build:watch",
    "format": "yarn format:base -w .",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "prepublishOnly": " cd ../../ && yarn copy && yarn build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:frames": "vitest run tests/frames",
    "test:intent": "vitest run tests/intent",
    "test:parsing": "vitest run tests/parsing",
    "test:prompt": "vitest run tests/prompt",
    "test:v2": "vitest run tests/V2",
    "test:v3": "vitest run tests/V3",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@circle-fin/developer-controlled-wallets": "^6.0.0",
    "@coinbase/cbpay-js": "^2.4.0",
    "@coinbase/coinbase-sdk": "0.11.2",
    "@redis/client": "^1.6.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.5",
    "openai": "^4.52.7",
    "typescript": "^5.4.5",
    "viem": "^2.16.3",
    "xmtp": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.2",
    "@vitest/coverage-v8": "^2.1.4",
    "prettier": "^3.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.2.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@circle-fin/developer-controlled-wallets": "^6.0.0",
    "@coinbase/cbpay-js": "^2.4.0",
    "@coinbase/coinbase-sdk": "0.11.2"
  },
  "peerDependenciesMeta": {
    "@circle-fin/developer-controlled-wallets": {
      "optional": true
    },
    "@coinbase/cbpay-js": {
      "optional": true
    },
    "@coinbase/coinbase-sdk": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.5.1",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
